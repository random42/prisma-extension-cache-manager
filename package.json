{
  "name": "prisma-extension-cache-manager",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Roberto Sero",
  "description": "Prisma client extension for caching model queries",
  "repository": {
    "type": "git",
    "url": "https://github.com/random42/prisma-extension-cache-manager.git"
  },
  "keywords": [
    "prisma",
    "@prisma/client",
    "extension",
    "cache",
    "caching"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "prepare": "run-s build",
    "build": "tsc",
    "prebuild": "rimraf dist",
    "prepublishOnly": "run-s test",
    "test": "node --test --test-only --require ts-node/register ./test/*.test.ts",
    "pretest": "prisma db push --force-reset --accept-data-loss --schema=./test/prisma/schema.prisma"
  },
  "engines": {
    "node": ">=18.18"
  },
  "peerDependencies": {
    "@prisma/client": "^6.0.1"
  },
  "devDependencies": {
    "@prisma/client": "^6.3.1",
    "cache-manager": "^6.4.0",
    "cacheable": "^1.8.8",
    "keyv": "^5.2.3",
    "npm-run-all": "^4.1.5",
    "prisma": "^6.3.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "object-code": "^1.3.3"
  }
}
